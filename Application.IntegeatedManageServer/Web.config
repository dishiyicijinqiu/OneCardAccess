      <register type="IPFBNSNRuleService" mapTo="PFBNSNRuleService">
        <lifetime type="singleton"/>
        <interceptor isDefaultForType="true" type="InterfaceInterceptor"/>
        <policyInjection />
      </register>
    </container>
  </unity>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="MyBehavior">
          <!-- To avoid disclosing metadata information, set the value below to false before deployment -->
          <serviceMetadata httpGetEnabled="true"/>
          <!-- To receive exception details in faults for debugging purposes, set the value below to true.  Set to false before deployment to avoid disclosing exception information -->
          <serviceDebug includeExceptionDetailInFaults="true"/>
          <unity containerName="ServicesContainer"/>
          <session />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <extensions>
      <behaviorExtensions>
        <add name="unity" type="FengSharp.OneCardAccess.Infrastructure.Services.UnityBehaviorElement, FengSharp.OneCardAccess.Infrastructure.Services"/>
        <add name="session" type="FengSharp.OneCardAccess.Infrastructure.Services.Session_Policy.SessionServerBehaviorElement, FengSharp.OneCardAccess.Infrastructure.Services"/>
      </behaviorExtensions>
    </extensions>
    <serviceHostingEnvironment multipleSiteBindingsEnabled="true" />
    <services>
      <service name="FengSharp.OneCardAccess.Domain.RBACModule.Service.AuthService" lockItem="False" behaviorConfiguration="MyBehavior">
